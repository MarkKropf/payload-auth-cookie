{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "dynamicImport": true
    },
    "target": "es2022",
    "loose": false,
    "keepClassNames": true
  },
  "module": {
    "type": "es6"
  },
  "minify": false,
  "sourceMaps": false
}
